(defun hex(s)
  (format nil"~{~2,'0x~}"(mapcar #'char-int(coerce s'list))))
(defun f(s p)
  (concatenate'string(subseq s 0 p)(hex s)(subseq s p)))
(princ(concatenate'string
       "#include<stdio.h>
int main(void){puts(\"console.log(`print\\\"puts\\\\\\\\\\\"print(bytes.fromhex('"
       (f""584)
       "').decode())\\\\\\\\\\\"\\\"`)\");}"))
